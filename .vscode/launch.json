    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Run Bot",
            "type": "python",
            "request": "launch",
            "program": "butler.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "run_bot",
                "-c",
                "configs/test",
                "--testing",
                "test-bot"
            ]
        },
        {
            "name": "Python: Run Server",
            "type": "python",
            "request": "launch",
            "program": "butler.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "run_server",
                "--skip-install-deps",
                //"-b"
            ]
        },
        {
            "name": "Python: Run Web",
            "type": "python",
            "request": "launch",
            "program": "butler.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "run_web",
                "--skip-install-deps",
            ]
        },
        {
            "name": "Python: Run Bootstrap",
            "type": "python",
            "request": "launch",
            "program": "butler.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "bootstrap",
            ]
        },
        {
            "name": "Python: Run Deploy",
            "type": "python",
            "request": "launch",
            "program": "butler.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": ["run", "setup", "--non-dry-run", "--local", "--config-dir=configs/test"]

        },
        {
            "name": "Python: Run Reproduce",
            "type": "python",
            "request": "launch",
            "program": "butler.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": ["reproduce", 
            "-t",
             "7bd76792-6f22-4afb-a3c7-35e87631820e",
              "-b",
               "test-bot/bot_working_directory/builds/test-release-build-bucket"
            ]

        },

    ]
}